{
  "name": "express-graphql-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "build": "tsc && yarn copy-graphql-files && yarn copy-js-files",
    "heroku-postbuild": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "copy-graphql-files": "copyfiles --flat src/graphql/*.graphql src/graphql/casosDeUso/**/*.graphql src/graphql/**/*.graphql src/graphql/entidades/**/*.graphql dist/src/graphql",
    "copy-js-files": "copyfiles -u 1 src/packages/*.js src/repositories/mongoose/plugins/*.js dist/src",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/introspection": "1.8.2",
    "@graphql-codegen/typescript": "1.8.2",
    "@graphql-codegen/typescript-resolvers": "1.8.2",
    "@playlyfe/gql": "^2.6.2",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-relay": "^0.4.11",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.7.8",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "copyfiles": "^2.1.1",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "graphql-toolkit": "^0.7.4",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.4",
    "async-redis": "^1.1.7",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "graphql": "^14.5.8",
    "graphql-relay": "^0.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.3",
    "schemalizer": "1.0.2"
  },
  "engines": {
    "node": "10.16.2",
    "yarn": "1.x"
  }
}
